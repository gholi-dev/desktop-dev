{
  "name": "liara",
  "productName": "liara",
  "version": "1.0.0",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 10 versions"
  ],
  "main": "backend/main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds",
    "builder": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --win --x64 --publish always"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@liara/cli": "^2.21.2",
    "@liara/platformicons": "^2.11.0",
    "@liara/react-drag-drop-files": "^2.2.4",
    "@sentry/electron": "^2.5.4",
    "@sentry/react": "^6.16.1",
    "ansi-up": "^1.0.0",
    "app-root-dir": "^1.0.2",
    "axios": "^0.24.0",
    "electron-updater": "^4.6.1",
    "get-port": "^5.1.1",
    "raven": "^2.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^16.0.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-download": "^4.1.1",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liara-cloud/desktop.git"
  }
}
